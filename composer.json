{
    "name": "drupal/drupal",
    "description": "Drupal 8 test project",
    "type": "project",
    "require": {
        "cweagans/composer-patches": "^1.6.0",
        "drupal/admin_toolbar": "^1.24",
        "drupal/advagg": "^3.5",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/block_field": "^1.0",
        "drupal/classy_paragraphs": "^1.0",
        "drupal/coffee": "^1.0",
        "drupal/colorbox": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6",
        "drupal/devel": "^2.0",
        "drupal/ds": "^3.3",
        "drupal/empty_page": "^2.0",
        "drupal/facets": "^1.3",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.0",
        "drupal/flag": "^4.0",
        "drupal/geolocation": "^3.0",
        "drupal/markdown": "^1.2",
        "drupal/metatag": "^1.8",
        "drupal/owlcarousel": "^1.0",
        "drupal/page_manager": "^4.0",
        "drupal/panels": "^4.4",
        "drupal/paragraphs": "^1.8",
        "drupal/parallax_bg": "^1.0",
        "drupal/photoswipe": "^2.6",
        "drupal/recaptcha": "^2.4",
        "drupal/rules": "^3.0",
        "drupal/search_api": "^1.12",
        "drupal/search_api_solr": "^2.7",
        "drupal/superfish": "^1.3",
        "drupal/swiftmailer": "^1.0",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.1",
        "drupal/views_slideshow": "^4.6",
        "drupal/webform": "^5.2",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "hussainweb/drupal-composer-helper": "^1.0"
    },
    "authors": [
        {
            "name": "Jey",
            "email": "y.mykhaliak@internetdevels.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "drupal-composer-helper": {
            "web-prefix": "htdocs"
        },
        "enable-patching": true,
        "installer-paths": {
          "htdocs/core": [
            "type:drupal-core"
          ],
          "htdocs/libraries/{$name}": [
            "type:drupal-library"
          ],
          "htdocs/modules/contrib/{$name}": [
            "type:drupal-module"
          ],
          "htdocs/profiles/contrib/{$name}": [
            "type:drupal-profile"
          ],
          "htdocs/themes/contrib/{$name}": [
            "type:drupal-theme"
          ],
          "htdocs/drush/contrib/{$name}": [
            "type:drupal-drush"
          ],
          "htdocs/modules/custom/{$name}": [
            "type:drupal-custom-module"
          ],
          "htdocs/themes/custom/{$name}": [
            "type:drupal-custom-theme"
          ]
        }
    },
    "prefer-stable": true,
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
